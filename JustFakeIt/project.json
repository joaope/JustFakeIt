{
    "version": "1.0.0",
    "authors": [ "JUST EAT plc" ],
    "copyright": "JUST EAT plc",
    "title": "JustFakeIt",
    "description": "An InProcess HTTP server which can be mocked and asserted against to allow for full stack HTTP testing",
    "buildOptions": {
        "warningsAsErrors": false,
        "platform": "anycpu",
        "languageVersion": "csharp6",
        "debugType": "portable"
    },
    "packOptions": {
        "owners": [ "JUST EAT plc" ],
        "summary": "An InProcess HTTP server which can be mocked and asserted against to allow for full stack HTTP testing",
        "tags": [ "owin", "http", "mock", "testing" ],
        "repository": {
            "type": "git",
            "url": "https://github.com/justeat/JustFakeIt.git"
        },
        "requireLicenseAcceptance": true,
        "licenseUrl": "https://github.com/justeat/JustFakeIt/blob/master/LICENSE.md",
        "projectUrl": "https://github.com/justeat/JustFakeIt"
    },
    "configurations": {
        "Debug": {},
        "Release": {
            "buildOptions": {
                "optimize": true
            }
        }
    },

    "dependencies": {
        "Microsoft.AspNetCore.Http.Abstractions": "1.0.0",
        "Microsoft.AspNetCore.Server.Kestrel": "1.0.0",
        "Newtonsoft.Json": "9.0.1"
    },

    "frameworks": {
        "net451": {}, 
        "netstandard13": {
            "dependencies": {
                "NETStandard.Library": "1.6.0",
                "System.Linq": "4.1.0",
                "System.Net.NetworkInformation": "4.1.0",
                "System.Net.WebHeaderCollection": "4.0.1"
            }
        }
    }
}